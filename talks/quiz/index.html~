<script>
var myDataRef = new Firebase('https://nitte-quiz.firebaseio.com/');
myDataRef.on('child_added', function(snapshot) {
  var slide_value = snapshot.val();
  if(slide_value == 'n'){
    Reveal.right(); 
  }
  if(slide_value == 'p'){
    Reveal.left(); 
  }

});
</script>

